{
  "expo": {
    "name": "it-americano",
    "slug": "it-americano",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/coffee.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/coffee.png",
        "backgroundColor": "#ffffff"
      },
      "package": "it.americano"
    },
    "web": {
      "favicon": "./src/assets/images/coffee.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/Fonts/Gilroy-Bold.otf",
            "./src/assets/Fonts/Gilroy-BoldItalic.otf",
            "./src/assets/Fonts/Gilroy-Medium.otf",
            "./src/assets/Fonts/Gilroy-MediumItalic.otf",
            "./src/assets/Fonts/Gilroy-SemiBold.otf",
            "./src/assets/Fonts/Gilroy-SemiBoldItalic.otf"
          ]
        }
      ]
    ],
    "extra": {
    }
  }
}
